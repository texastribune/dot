{
  "name": "dot",
  "description": "The Texas Tribune's pixel tracking tool",
  "scripts": {
    "tsc:server": "rimraf build && tsc -b tsconfig.server.json",
    "start": "nodemon --watch server --ext ts --exec ts-node --project tsconfig.server.json server/app.ts",
    "start:prod": "NODE_ENV=production node build/app.js",
    "build": "npm run tsc:server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/texastribune/dot"
  },
  "bugs": {
    "url": "https://github.com/texastribune/dot/issues"
  },
  "dependencies": {
    "express": "^4.17.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^12.12.35",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "license": "MIT"
}
