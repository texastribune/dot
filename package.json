{
  "name": "dot",
  "description": "The Texas Tribune's pixel tracking tool",
  "version": "2.8.10",
  "scripts": {
    "build": "npm run build:dashboard && npm run build:server",
    "build:dashboard": "rimraf dist && NODE_ENV=production webpack",
    "build:server": "rimraf build && tsc -b server/tsconfig.json",
    "lint": "eslint **/*.{ts,js,vue}",
    "release": "release-it",
    "start:watch": "nodemon --watch server --ext ts --exec ts-node --project server/tsconfig.json server/app.ts",
    "start": "node build/server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/texastribune/dot"
  },
  "bugs": {
    "url": "https://github.com/texastribune/dot/issues"
  },
  "dependencies": {
    "@mdi/js": "^5.4.45",
    "@sentry/browser": "^5.22.3",
    "@sentry/integrations": "^5.22.3",
    "@sentry/node": "^5.15.4",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "auth0-js": "^9.13.3",
    "axios": "^0.21.2",
    "connect-slashes": "^1.4.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-jwt": "^6.0.0",
    "google-spreadsheet": "^3.1.15",
    "graphql": "^14.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "helmet": "^3.23.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "pg": "^8.2.0",
    "pg-hstore": "^2.3.3",
    "pug": "^3.0.1",
    "sequelize": "^5.21.8",
    "statuses": "^2.0.0",
    "validator": "13.7.0",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.4",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.4",
    "vuex": "^3.5.1",
    "webpack": "^4.42.1",
    "webpack-dev-middleware": "^3.7.2"
  },
  "devDependencies": {
    "@types/auth0-js": "^9.12.4",
    "@types/bluebird": "^3.5.30",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/google-spreadsheet": "^3.0.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.0",
    "@types/node": "^12.12.35",
    "@types/validator": "10.11.0",
    "@types/webpack": "^4.41.11",
    "@types/webpack-dev-middleware": "^3.7.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "css-loader": "^3.5.2",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.3.0",
    "nodemon": "^2.0.3",
    "release-it": "^14.12.3",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typescript": "^4.6.2",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.11"
  },
  "license": "MIT",
  "release-it": {
    "hooks": {
      "after:bump": "cd analytics && mkdir ${version} && cd ../ && cp tracker/pixel.js analytics/${version} && git add analytics/${version}/pixel.js"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
